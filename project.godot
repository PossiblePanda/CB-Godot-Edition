config_version=5

[application]

config/name="Containment Breach: Godot Edition"
run/main_scene="res://scenes/menus/loading.tscn"
config/custom_user_dir_name="CB_GodotEdition"
config/features=PackedStringArray("4.4", "Forward Plus")
run/max_fps=165
config/icon="res://icon.png"

[autoload]

ModLoaderStore="*res://addons/mod_loader/mod_loader_store.gd"
ModLoader="*res://addons/mod_loader/mod_loader.gd"
Global="*res://data/singletons/global.gd"
LoadManager="*res://data/singletons/load_manager.gd"
Utils="*res://data/singletons/utils.gd"
DiscordRPCLoader="*res://addons/discord-rpc-gd/nodes/discord_autoload.gd"
Config="*res://data/singletons/config.gd"
GlobalUi="*res://data/singletons/global_ui.gd"
LimboConsole="*res://addons/limbo_console/limbo_console.gd"
BuiltinCommandsRegister="*res://data/singletons/builtin_commands_register.gd"
Register="*res://data/singletons/register.gd"
SaveManager="*res://data/singletons/save_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/initial_position_type=2
window/size/borderless=true
window/size/extend_to_title=true
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/discord-rpc-gd/plugin.cfg", "res://addons/limbo_console/plugin.cfg")

[filesystem]

import/blender/enabled=false

